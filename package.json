{
    "name": "react-hackernews",
    "version": "1.0.2",
    "description": "HackerNews as a React component",
    "keywords": [
        "hackernews",
        "hacker news",
        "newsreader",
        "react news"
    ],
    "license": "MIT",
    "main": "./dist/index.js",
    "scripts": {
        "start": "CHOKIDAR_USEPOLLING=true webpack serve --mode development --env development --config webpack.dev.js",
        "build": "yarn cleanup && webpack --config webpack.prod.js",
        "build:dev": "yarn cleanup && webpack --config webpack.dev.js",
        "cleanup": "rimraf dist && rimraf build",
        "test": "jest",
        "lint": "eslint src/"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "dayjs": "^1.10.4",
        "prop-types": "^15.7.2"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.16",
        "@babel/core": "^7.14.0",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-transform-runtime": "^7.13.15",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.14.1",
        "@babel/preset-react": "^7.13.13",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "concurrently": "^6.1.0",
        "copy-webpack-plugin": "^8.1.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "enzyme-to-json": "^3.6.2",
        "eslint": "^7.26.0",
        "eslint-config-babel": "^9.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-standard": "^5.0.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.1",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hot-loader": "^4.13.0",
        "rimraf": "^3.0.2",
        "terser-webpack-plugin": "^5.1.1",
